#%RAML 1.0 DataType

    type: object
    properties:
      productid:
       required: true
       type: integer

      categoryid:
        required: true
        type: integer
      name:
        required: true
        type: string
      description: 
       required: true
       type: string
      video_url:
        required: true
        type: string
        pattern: 'https?://.+'
      price:
        required: true
        type: integer