#%RAML 1.0 SecurityScheme
type: OAuth 2.0
description: This API support oauth security
describedBy:
  headers:
    Authorization:
      description: User have to send token to the server for any request
      type: string
  queryParameters:
    access_token:
      description: user have to send valid access token
      type: string
  responses:
    401:
      description: unauthorized user
    403:
      description: forbidden
      
  